[package]
name = "unzipper"
version = "0.1.0"
authors = ["turgu1"]
description = "A library for Zip file extraction."
readme = "README.md"
repository = "https://github.com/turgu1/unzipper"
keywords = ["zip", "unzip", "archive", "file extraction"]
edition = "2021"
license = "MIT"

[dependencies]
log = "0.4.27"
miniz_oxide = { version = "0.8.8", default-features = false }

[dev-dependencies]
test_support = { path = "./tests/test_support" }
